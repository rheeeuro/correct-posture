{
  "name": "correct-posture",
  "version": "1.0.0",
  "description": "Web project for correcting false posture using poseNet(TensorFlow)",
  "main": "index.js",
  "author": "Rhee Euro",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "ngrok": "^3.2.7",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-naver": "^1.0.6",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.1",
    "sass-loader": "^8.0.2",
    "shx": "^0.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' --ignore 'files'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/files','src/webpack.config.js'",
    "copyAll": "shx cp -r src/static build/static && shx cp -r src/views build/views && shx cp -r src/files build/files",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "buildWindow": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuildWindow": "rd /s /q build",
    "heroku-prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build",
    "tunnel": "ngrok http 4000",
    "test": "node_modules/.bin/mocha -r @babel/register .test.js"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/register": "^7.10.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
